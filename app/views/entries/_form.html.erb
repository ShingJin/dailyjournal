$(document).ready(function(){

$('#case_first_list').dblclick(function(){
    assignList();
  });
  $('#case_second_list').dblclick(function(){
    unassignList();
  });

  $('#contact_first_list').dblclick(function(){
    assignListCon();
  });
  $('#contact_second_list').dblclick(function(){
