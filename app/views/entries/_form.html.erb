});



    </script>


<%= form_for @entry, :html => { :class => "form-horizontal entry" } do |f| %>

  <% if @entry.errors.any? %>
    <div id="error_expl" class="panel panel-danger">
      <div class="panel-heading">
        <h3 class="panel-title"><%= pluralize(@entry.errors.count, "error") %> prohibited this entry from being saved:</h3>
      </div>
      <div class="panel-body">
        <ul>
        <% @entry.errors.full_messages.each do |msg| %>
          <li><%= msg %></li>
        <% end %>
        </ul>
      </div>
    </div>
  <% end %>

  <div class="form-group">
    <%= f.label :entry_text, :class => 'control-label col-lg-2' %>
    <div class="col-lg-10">
      <%= f.text_field :entry_text, :class => 'form-control' %>
    </div>
  </div>

  <div class="form-group">

 <%= f.label :cases, :class => 'control-label col-lg-2' %>
    <div class="col-lg-10">

   <select id="case_first_list" multiple="multiple">
    <% for c in @cases %>
      <option selected="selected" value=<%= c.id %>><%= c.name %>
      </option> 
    <% end %>
  </select>
  <select id="case_second_list" multiple="multiple" name="cases" style="width:35px;">
  </select>

</div>
  </div>

  <div class="form-group">

<%= @tags.to_yaml %>

 <%= f.label :contacts, :class => 'control-label col-lg-2' %>
    <div class="col-lg-10">

   <select id="contact_first_list" multiple="multiple">
    <% for c in @contacts %>
      <option selected="selected" value=<%= c.id %>><%= c.name %>
      </option> 
    <% end %>
  </select>
   <select id="contact_second_list" multiple="multiple" name="contacts" style="width:70px;">
  </select>
</div>
  </div>

    <div class="form-group">

 <%= f.label :tags, :class => 'control-label col-lg-2' %>
    <div class="col-lg-10">

   <select id="tag_first_list" multiple="multiple">
    <% for t in @tags %>
      <option selected="selected" value=<%= t.id %>><%= t.name %>
      </option> 
    <% end %>
  </select>

     <select id="tag_second_list" multiple="multiple" name="tags" style="width:85px;">
    </select>



</div>
  </div>



  <div class="form-group">
    <div class="col-lg-offset-2 col-lg-10">
      <%= f.submit nil, :class => 'btn btn-primary' %>
      <%= link_to t('.cancel', :default => t("helpers.links.cancel")),
                entries_path, :class => 'btn btn-default' %>
    </div>
  </div>

<% end %>
